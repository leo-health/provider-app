  /*Glyph-Icons*/
  .glyphicon-star:before {
    color: #FC0;
    content: "\e006";
  }
  .glyphicon-ok-sign:before {
    color: #0B2;
    content: "\e084";
  }
  .glyphicon-exclamation-sign:before {
    color:#f00;
    content: "\e101";
  }
  .glyphicon-exclamation-sign-default:before {
    content: "\e101";
  }

  /*Blockquote*/
  blockquote footer:before,
	blockquote small:before,
	blockquote .small:before {
	  content: "";
	}

  /*Registration*/
  .right-fix {
    padding-right: 2em;
  }

  .page-header {
    padding-top: .5%;
  }

  .patient-avatar {
    height: 3.5em;
    margin-top: 0.2em;
    margin-right: 1.5em;
  }

  .registration-icon {
    font-size: 20px;
    padding: 3px;
  }

  .well.registration-well {
    background-color: #fff;
  }

  .well.faq-well {
   background-color: #fff;
   border-radius: 2px;
   border-color: #eee;
  }

  /*Tab styling*/
  .tags-container { width: 100%; }

  @media (min-width: 1200px) {
    .tags-container { width: 24%; }
  }

  .assignment-font { font-family: "Futura PT W01 Book" }

  .active-tab > .tab-name,
  .nav-tabs > li.active-tab > .tab-name:focus,
  .nav-tabs > li.active-tab > .tab-name:hover,
  .nav-tabs > li.active-tab > .tab-name:focus:hover {
    color: white;
    box-shadow: none;
  }

  .inactive > .tab-name,
  .inactive > .tab-name:hover { box-shadow: none; }

  .open-case,
  .escalated,
  .closed {
    width: 33%;
    text-align: center;
    height: 44px;
  }

  .open-div,
  .escalated-div,
  .closed-div {
    width: 100%;
    height: 10px;
  }

  .tab-name {
    padding-top: 10px;
    height: 100%;
  }

  .open-div,
  .active-tab.open-case { background-color: #77DD77 }

  .escalated-div,
  .active-tab.escalated { background-color: #FFB347 }

  .closed-div,
  .active-tab.closed { background-color: #696969 }
}

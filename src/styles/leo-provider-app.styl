@css {
  /*Global styling*/
  .dark-gray-font { color: #575757 }
  .orange-font { color: #FF906A }
  .heavy-font-size {
    font-family: "Futura PT W01 Medium";
    font-size: 18px !important;
  }

  .medium-font-size {
    font-family: "Futura PT W01 Book";
    font-size: 18px !important;
  }

  /*Navbar*/
  .leo-logo {
    font-size: 40px;
    display: inline;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
  }

  .logout-nav {
    margin-right: 20px;
    margin-top: 8px;
  }

  .leo-nav {
    margin-left: 30px;
    width: 30%;
  }

  .leo-logo-orange { color: #FF906A }

  .leo-logo-gray { font-family: "Futura PT W01 Book" }

  .logout-nav > li > .navbar-welcome { padding-right: 15px; }

  /*Positioning*/
  .message-container { margin-top: -140px }

  #all-tab { height: 100% }

  .tags-container { margin-bottom: 40px }

  .conversations-container {
    height: 655px;
    z-index: 1;
  }

  .find-family-container { margin-top: -10px }

  .find-family-container > .panel{
    padding-bottom: 5px;
    margin-bottom: 18px;
  }
  .notes-container {
    margin-top: -140px;
    height: 780px;
  }

  #chatbox {
    height: 610px;
    z-index: 1;
  }

  #staff-selection {
    position: absolute;
    margin-top: 55px;
    margin-left: -415px;
    z-index: 2;
  }

  footer { margin: 0px }

  .main-container { height: 88vh }

  /*Tab styling*/
  .tags-container {
    width: 420px;
  }

  .assignment-font { font-family: "Futura PT W01 Book" }

  .active-open > a,
  .active-escalated > a,
  .active-closed > a {
    color: white !important;
    box-shadow: none !important;
    &: hover {
      box-shadow: none !important;
    }
  }

  .open-case,
  .escalated,
  .closed {
    width: 33% !important;
    text-align: center !important;
    height: 44px !important;
  }

  .open-div,
  .escalated-div,
  .closed-div {
    width: 100% !important;
    height: 10px !important;
  }

  .open-div,
  .active-open {
    background-color: #77DD77
  }

  .inactive-open > a,
  .inactive-escalated > a,
  .inactive-closed > a {
    box-shadow: none !important;
    &: hover {
      box-shadow: none !important;
    color: #575757 !important;
    }
  }

  .tab-name {
    padding-top: 10px !important;
    height: 100% !important;
  }

  .escalated-div,
  .active-escalated {
    background-color: #FFB347
  }

  .closed-div,
  .active-closed {
    background-color: #696969
  }

  /*Messaging & Conversations*/
  #staff-selection { margin-bottom: -10px}
  #chatbox { position: relative; overflow-x: hidden; width: 100%; }
  #chatmessages { bottom: 3%; position: absolute; overflow: auto; width: 98%; max-height: 700px }
  .img-chat { max-width:400px; max-height:200px; width: auto; height: auto; }
  .img-wrapper {height: 200px;}
  .img-helper {display: inline-block; height: 100%; vertical-align: middle;}
  .child-label { margin-right: 0.5em; display: inline-block }
  .message-button { margin-right: 0.5em }
  .guardian-name { background-color: red }
  .progress-table {display: table; width: 100%; }
  #signup_page {padding-top: 10px}
  #signup_logo { float: left; display: inline; vertical-align: middle }
  #signup_progress{ overflow: hidden}
  #progress { padding-left: 60px }
  .progress-text {color: black; font-size: 18px; font-weight: 400}
  .signup_header{font-weight: 100}
  .progress-text-container{width: 15%}
  .signup-progress-text,
  .progress-text-spacer{display: table-cell; float: none; padding: 0 0 14px; vertical-align: bottom; horizontal-align: middle}
  .full-width-button{width: 100%}
  p.signup-progress-text.progress-text-container {
    padding:0 2em 1em;
  }
  @media (min-width: 650px) {
    #progress_xs {display: none};
  }
  @media (max-width: 650px) {
    #progress {display: none};
  }
  .message-cell { margin-right: 1em; }
  /*Inline HR for System Messages*/
  .inline-hr {
      display: block;
      text-align: center;
      overflow: hidden;
      white-space: nowrap;
  }

  .inline-hr > span {
      position: relative;
      display: inline-block;
  }

  .inline-hr > span:before,
  .inline-hr > span:after {
      content: "";
      position: absolute;
      top: 50%;
      width: 9999px;
      height: 1px;
      background: #eee;
  }

  .inline-hr > span:before {
      right: 100%;
      margin-right: 15px;
  }

  .inline-hr > span:after {
      left: 100%;
      margin-left: 15px;
  }
  .inline-hr .danger {
    color: #cb171e;
  }
  .inline-hr .primary {
    color: #0d87e9;
  }

  /*React AutoSuggest*/
  .react-autosuggest { position: relative; font-family: "Futura PT W01 Medium" }
  .react-autosuggest input { width: 100%; height: 95% }
  .react-autosuggest__suggestions{ z-index: 10000; position:absolute; background-color: #fff; width: 100%; border: 1px solid #aaaaaa; }
  .react-autosuggest ul { list-style: none; padding-left: 0; }
  .react-autosuggest li { margin-left: 0; }
  .react-autosuggest__suggestion--focused { background-color: #ddd; }
  .react-autosuggest__suggestions-section-name { font-size: 1.0em; text-transform: uppercase; color: #777; margin-left: 5% ; font-family: "Futura PT W01 Medium" }
  .react-autosuggest__suggestion { padding-left: 15%; font-family: "Futura PT W01 Medium" }

  /*TypeAhead*/
  span.twitter-typeahead { width: 100%; }
  .input-group span.twitter-typeahead { display: block !important; }
  .input-group span.twitter-typeahead .tt-dropdown-menu { top: 32px !important; }
  .input-group.input-group-lg span.twitter-typeahead .tt-dropdown-menu { top: 44px !important; }
  .input-group.input-group-sm span.twitter-typeahead .tt-dropdown-menu { top: 28px !important; }
  .tt-hint { color: #ddd; }
  .tt-menu { padding: 8px 0; background-color: #fff; border: 1px solid #ccc; border: 1px solid rgba(0, 0, 0, 0.2); border-radius: inherit; box-shadow: 0 5px 10px rgba(0,0,0,.2); }
  .tt-suggestion { padding: 3px 20px; font-size: 18px; line-height: 24px; }

  /*Alert*/
  .alert-form { color: #fff!important; font-size:16px; }
  .alert-form .form-control { color: #fff; }
  .alert-default { background-color: #bbbbbb; border-color: #d6e9c6; color: #666666 !important; }
  .alert-default .alert-link { color: #666666; }

  /*Pre-scrollable Content*/
  .pre-scrollable {
    min-height: 100%;
    max-height: 100%;
    overflow-y: auto;
  }
  .pre-scrollable-left {
    overflow-y: auto;
  }
  .container {
    margin-right: auto;
    margin-left: auto;
    padding-left: 20px;
    padding-right: 20px;
  }

  /*Media Queries - Reset*/
  @media (min-width: 768px) {
    .container {
      width: initial;
    }
  }
  @media (min-width: 992px) {
    .container {
      width: initial;
    }
  }
  @media (min-width: 1200px) {
    .container {
      width: initial;
    }
  }


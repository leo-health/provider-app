  /*Glyph-Icons*/
  .glyphicon-star:before {
    color: #FC0;
    content: "\e006";
  }
  .glyphicon-ok-sign:before {
    color: #0B2;
    content: "\e084";
  }
  .glyphicon-exclamation-sign:before {
    color:#f00;
    content: "\e101";
  }
  .glyphicon-exclamation-sign-default:before {
    content: "\e101";
  }

  /*Blockquote*/
  blockquote footer:before,
	blockquote small:before,
	blockquote .small:before {
	  content: "";
	}

  /*Registration*/
  .page-header {
    padding-top: .5%;
  }

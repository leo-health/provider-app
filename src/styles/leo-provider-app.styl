  /*Glyph-Icons*/
  .glyphicon-star:before {
    color: #FC0;
    content: "\e006";
  }
  .glyphicon-ok-sign:before {
    color: #0B2;
    content: "\e084";
  }
  .glyphicon-exclamation-sign:before {
    color:#f00;
    content: "\e101";
  }
  .glyphicon-exclamation-sign-default:before {
    content: "\e101";
  }

  /*Blockquote*/
  blockquote footer:before,
	blockquote small:before,
	blockquote .small:before {
	  content: "";
	}

  /*Registration*/
  .right-fix {
    padding-right: 2em;
  }

  .page-header {
    padding-top: .5%;
  }

  .patient-avatar {
    height: 3.5em;
    margin-top: 0.2em;
    margin-right: 1.5em;
  }

  .registration-icon {
    font-size: 20px;
    padding: 3px;
  }

  .well.registration-well {
    background-color: #fff;
  }

  .well.faq-well {
   background-color: #fff;
   border-radius: 2px;
   border-color: #eee;
  }
}
